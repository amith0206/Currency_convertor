{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "rewrites": [
      { "source": "/home", "destination": "/home.html" },
      { "source": "/algorithm", "destination": "/algorithm.html" },
      { "source": "/product", "destination": "/product.html" }
    ]
  }
  
  